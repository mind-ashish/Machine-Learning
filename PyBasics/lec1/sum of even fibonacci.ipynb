    "n=int(input())\n",
    "t=[1,1]\n",
    "i,sum=0,0\n",
    "j=t[i]+t[i+1]\n",
    "while j<=n :\n",
    "    if j&1==0:\n",
    "        sum+=j\n",
    "    t.append(j)\n",
    "    i+=1\n",
    "    j=t[i]+t[i+1]\n",
    "print(sum)\n"
   ]
